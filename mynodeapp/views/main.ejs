<!DOCTYPE html>
<html lang="en">

<head>
  <% include head %>
</head>

<body>
  <header>
    <% include header %>
  </header>

  <div class="container-fluid bg-3 text-center">
    <section>
      <% include searchbar %>
    </section>

    <br />
    <div class="container">
      <div id= "content">
        <table class="table table-striped table-primary">
            <thead>
              <tr>
                <th>Judul</th>
                <th>Genre</th>
                <th>Tahun</th>
                <th>Rating</th>
                <th>Sinopsis</th>
              </tr>
            </thead>
            <tbody>
              <% value.forEach( ploop => { %>
              <tr>
                <td><%= ploop.judul.value %></td>
                <td><%= ploop.genre.value %></td>
                <td><%= ploop.tahun.value %></td>
                <td><%= ploop.rating.value %></td>
                <td><%= ploop.sinopsis.value %></td>
              </tr>
              <% })
              %>
            </tbody>
        </table>
      </div>
    </div>
  </div>
  <% include footer %>
</body>

</html>